<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uptop.healthup"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:icon="@drawable/healthup_icon600x300_new"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:allowBackup="true" >
        <activity
            android:name="com.uptop.healthup.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.uptop.healthup.SignUp"></activity>
        <activity android:name="com.uptop.healthup.ConfirmSignUp"></activity>
        <activity android:name="com.uptop.healthup.Profile"></activity>
        <activity android:name="com.uptop.healthup.AdvancedProfile"></activity>
        <activity android:name="com.uptop.healthup.Home"></activity>
    </application>

</manifest>